{
  "name": "easy-e2e-mobile",
  "version": "1.0.0",
  "private": true,
  "repository": "https://github.com/arhs-spikeseed-mobile/easy-e2e-mobile.git",
  "author": "Lorenzo Greco <lorenzo.greco@arhs-spikeseed.com>",
  "license": "MIT",
  "scripts": {
    "e2e:local:prebuild": "echo \"Your prebuild command here\"",
    "e2e:browserstack:prebuild": "echo \"Your prebuild command here\"",
    "e2e:local:build:ios": "yarn e2e:local:prebuild && sh lib/scripts/e2e.local.ios.sh --build",
    "e2e:local:build:android": "yarn e2e:local:prebuild && sh lib/scripts/e2e.local.android.sh --build",
    "e2e:local:run:ios": "concurrently -P \"yarn _appium\" \"sleep 5 && sh lib/scripts/e2e.local.ios.sh --run {@}\" -k --",
    "e2e:local:run:android": "concurrently -P \"emulator -avd Pixel_2_API_31\" \"yarn _appium\" \"sleep 5 && sh lib/scripts/e2e.local.android.sh --run {@}\" -k --",
    "e2e:browserstack:build:ios": "yarn e2e:browserstack:prebuild && sh lib/scripts/e2e.browserstack.ios.sh --build",
    "e2e:browserstack:build:android": "yarn e2e:browserstack:prebuild && sh lib/scripts/e2e.browserstack.android.sh --build",
    "e2e:browserstack:run:ios": "sh lib/scripts/e2e.browserstack.ios.sh --run --",
    "e2e:browserstack:run:android": "sh lib/scripts/e2e.browserstack.android.sh --run --",
    "_appium": "npx appium --allow-cors"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.18.9",
    "@wdio/appium-service": "^7.21.0",
    "@wdio/cli": "^7.21.0",
    "@wdio/local-runner": "^7.21.0",
    "@wdio/mocha-framework": "^7.21.0",
    "@wdio/spec-reporter": "^7.21.0",
    "appium": "^1.22.3",
    "browserstack-local": "^1.5.1",
    "chromedriver": "^104.0.0",
    "concurrently": "^7.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "wdio-chromedriver-service": "^7.3.2"
  }
}
